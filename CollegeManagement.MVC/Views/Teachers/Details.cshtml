@model CollegeManagement.Model.Teacher

@{
    ViewBag.Title = "Teacher Details";
}

<h2>Teacher Details</h2>

<div>
    <h4>Teacher</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Teacher Name
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TeacherName)
        </dd>

        <dt>
            Teacher Birth Date
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TeacherBirthDate)
        </dd>

        <dt>
            Teacher Salary
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TeacherSalary)
        </dd>
    </dl>
</div>
<button type="button" class="btn btn-default">@Html.ActionLink("Edit", "Edit", new { id = Model.TeacherId })</button>
<button type="button" class="btn btn-default">@Html.ActionLink("Back to List", "Index")</button>